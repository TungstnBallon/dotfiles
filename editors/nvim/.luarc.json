{
	"$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
	"workspace": {
		"library": [
			"$VIMRUNTIME",
			"$HOME/.local/share/nvim/site/pack/core/opt"
		],
		"checkThirdParty": "false"
	},
	"runtime": {
		"path": [
			"lua/?.lua",
			"lua/?/init.lua"
		],
		"version": "LuaJIT"
	}
}
